"UnlitGeneric"
{
	"$basetexture" "models/weapons/slipgun/zmb_slipgun_scroll"
	"$angle" 0.0
	"$translate" "[0 0]"
	"$center" "[0.5 0.5]"

	"Proxies"
	{
		"LinearRamp"
		{
		"rate" 1
		"initialValue" 0.0
		"resultVar" "$angle"
		}

		"TextureTransform"
		{
		"translateVar" "$translate"
		"rotateVar" "$angle"
		"centerVar" "$center"
		"resultVar" "$basetexturetransform"
		}
	}
}
