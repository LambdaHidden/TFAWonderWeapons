"VertexlitGeneric"
{
	"$basetexture" "models/weapons/raygun/~-gzombie_teleporter_glow_c"
	"$surfaceprop" "Default"
	
	"$translate" "[0]"
	"$center" "[0.5 0.5]"

	"Proxies"
	{
		"LinearRamp"
		{
		"rate" 1
		"initialValue" 0.0
		"resultVar" "$translate"
		}

		"TextureTransform"
		{
		"translateVar" "$translate"
		"centerVar" "$center"
		"resultVar" "$basetexturetransform"
		}
	}
}
